      "####################\n",
      "# OUTPUT:          #\n",
      "#                  #\n",
      "# January 27, 1756 #\n",
      "# Mozart           #\n",
      "####################\n"
     ]
    }
   ],
   "source": [
    "txt = \"\"\"January 27, 1756\n",
    "Mozart\"\"\"\n",
    "\n",
    "lines = txt.split(\"\\n\")\n",
    "width = len(max(lines, key=len))\n",
    "width = max(7, width)\n",
    "lines = [f\"# {line:<{width}} #\" for line in [\"OUTPUT:\",\"\"] + lines]\n",
    "\n",
    "output = f'{\"#\"*(width+4)}\\n{chr(10).join(lines)}\\n{\"#\"*(width+4)}'\n",
    "print(output)"
   ]
